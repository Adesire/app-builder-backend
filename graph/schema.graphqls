type Passphrase {
  host String
  view String
}

type Session { 
  channel: String
  rtc: String!
  rtm: String!
  uid: Int!
  host: Boolean!
}

type Query {
  joinChannel(channel: String!, password: String!): Session!
  joinChannelWithPassphrase(passphrase: Passphrase): Session!
}

type Mutation {
  login(token: String!): String!
  createChannel(channel: String!, hostPassword: String, viewPassword: String): Passphrase!
}